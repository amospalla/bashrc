# FileVersion=2

[calendars]

[[calendari_jordi_local]]
path = ~/.calendars/*
type = discover

[[contactes_jordi_local]]
path = ~/.contacts/*
type = discover

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M
